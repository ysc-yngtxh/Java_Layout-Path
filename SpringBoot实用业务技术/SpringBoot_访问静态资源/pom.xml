<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>3.4.5</version>
        <relativePath/> <!-- lookup parent from repository -->
    </parent>

    <groupId>com.example</groupId>
    <artifactId>SpringBoot_AccessStaticResource</artifactId>
    <version>0.0.1-SNAPSHOT</version>
    <name>SpringBoot_访问静态资源</name>
    <description>SpringBoot_访问静态资源</description>

    <modules>
        <module>01_SpringBoot-StaticLocal-Properties</module>
        <module>02_SpringBoot-StaticLocal-Config</module>
        <module>03_SpringBoot-Properties-Config</module>
        <module>04_SpringBoot-Templates-Controller</module>
    </modules>
    <packaging>pom</packaging>

    <properties>
        <java.version>23</java.version>
    </properties>

    <dependencies>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-web</artifactId>
        </dependency>
    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-maven-plugin</artifactId>
            </plugin>
        </plugins>
